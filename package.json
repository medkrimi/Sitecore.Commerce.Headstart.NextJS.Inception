    "@types/date-fns": "^2.6.0",
    "apexcharts": "^3.36.0",
    "axios": "^0.27.2",
    "chart.js": "^3.9.1",
    "formik": "^2.2.9",
    "framer-motion": "^7.6.4",
    "next": "12.2.0",
    "next-seo": "^5.4.0",
    "ordercloud-javascript-sdk": "^4.10.1",
    "pre-commit": "^1.2.2",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
